{
  "name": "ChatGPT PromptJump",
  "version": "1.2.0",
  "manifest_version": 3,
  "description": "PromptJump â€” jump to your prompts in active chat sessions on ChatGPT.",
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "chat-manager.js",
        "xhr-tracker.js",
        "api-tracker.js"
      ],
      "matches": [
        "https://chatgpt.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": ["main-script.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/logo-16.png",
      "48": "icons/logo-48.png",
      "128": "icons/logo-128.png"
    },
    "default_title": "ChatGPT PromptJump",
    "default_popup": "popup.html"
  }
}
